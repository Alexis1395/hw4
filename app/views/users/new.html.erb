<main class="container">
<h1 class="mb-4">New User Account</h1>

<form action="/users" method="post" class="needs-validation" novalidate>
  <div class="mb-3">
    <label for="first_name_input" class="form-label"> First Name</label>
    <input type="text" name="first_name" id="first_name"  class="form-control">
  </div>

  <div class="mb-3">
    <label for="last_name_input" class="form-label">Last Name</label>
    <input type="text" name="last_name" id="first_name"  class="form-control">
  </div>

  <div class="mb-3">
    <label for="email_input" class="form-label">Email</label>
    <input type="text" name="email" id="email"  class="form-control">
  </div>
  
  <div class="mb-3">
    <label for="password_input" class="form-label">Password</label>
    <input type="text" name="password" id="password"  class="form-control">
  </div>

  <button class="btn btn-primary">Sign Up</button>
</form>
</main>


